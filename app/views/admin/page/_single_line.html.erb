<div class="requisitionModel J_single_line" id="div_<%= id%>" onMouseOut="in_element('<%= id%>')" onMouseOver="out_element('<%= id%>')">
	<input type="hidden" name="list[<%= id%>]type" value="single_line"/>
	<input type="hidden" name="list[<%= id%>]must" id="<%= id%>_must" value="<%= hash.blank? ? 0 : hash["must"]%>"/>
	<input type="hidden" name="list[<%= id%>]title" id="<%= id%>_title" value="<%= hash.blank? ? "" : hash["title"]%>"/>
	<input type="hidden" name="list[<%= id%>]size" id="<%= id%>_size" value="<%= hash.blank? ? 20 : hash["size"]%>"/>
	<p class="requisitionLabel_text text_color"><%= hash.blank? ? "" : hash["title"]%></p>
	<label class="requisitionLabel">
		<input type="text" class="requisitionAge input_text" />
	</label>
	<% if hash.blank? || hash["must"] == "0"%>
		<span class="required">*</span>
	<% else %>
		<span class="required" style="display: none;">*</span>
	<% end %>
	<div class="addOperating">
		<div class="requisitionModel_bg"></div>
		<span class="iconRequisition icon_move_model"></span>
		<a href="javascript:del_element('<%= id%>');" class="iconRequisition icon_delete_model J_delete_model"></a>
		<a href="<%= manager_link("/admin/page/edit_text/#{id}?must=#{hash.blank? ? 0 : hash["must"]}")%>" data-remote="true" class="iconRequisition icon_editor_model J_editor_single_line">编辑</a>
	</div>
</div>