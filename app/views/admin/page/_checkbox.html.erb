<div class="requisitionModel J_checkbox" id="div_<%= id%>" onMouseOut="in_element('<%= id%>')" onMouseOver="out_element('<%= id%>')">
  <input type="hidden" name="list[<%= id%>]type" value="checkbox"/>
  <input type="hidden" name="list[<%= id%>]must" id="<%= id%>_must" value="<%= hash.blank? ? 0 : hash["must"]%>"/>
  <input type="hidden" name="list[<%= id%>]size" id="<%= id%>_size" value="<%= hash.blank? ? 0 : hash["size"]%>"/>
  <input type="hidden" name="list[<%= id%>]title" id="<%= id%>_title" value="<%= hash.blank? ? "请输入控件名称，最多不超过12个字：" : hash["title"]%>"/>
  <div id="values_<%= id%>">
    <% if hash.blank?%>
      <input type="hidden" name="list[<%= id%>]value[]" class="values" value="选项"/>
      <input type="hidden" name="list[<%= id%>]value[]" class="values" value="选项"/>
    <% else %>
      <% hash["value"].each do |val|%>
        <input type="hidden" name="list[<%= id%>]value[]" class="values" value="<%= val%>"/>
      <% end %>
    <% end %>
  </div>
	<p class="requisitionLabel_text text_color"><%= hash.blank? ? "请输入控件名称，最多不超过12个字：" : hash["title"]%></p>
  <div id="value_inputs_<%= id%>">
    <% if hash.blank?%>
      <label class="requisitionLabel requisitioncheckbox"><input type="checkbox" class="requisitionFind" name="iluopan" />选项</label>
      <label class="requisitionLabel requisitioncheckbox"><input type="checkbox" class="requisitionFind" name="iluopan" />选项</label>  
    <% else %>
      <% hash["value"].each do |val|%>
        <label class="requisitionLabel requisitioncheckbox"><input type="checkbox" class="requisitionFind" name="iluopan" /><%= val%></label>
      <% end %>
    <% end %>
  </div>
  <% if hash.blank? || hash["must"] == "0"%>
    <span class="required">*</span>
  <% else %>
    <span class="required" style="display: none;">*</span>
  <% end %>
  <div class="addOperating">
  	<div class="requisitionModel_bg"></div>
  	<span class="iconRequisition icon_move_model"></span>
  	<a href="javascript:del_element('<%= id%>');" class="iconRequisition icon_delete_model J_delete_model"></a>
  	<a href="<%= manager_link("/admin/page/edit_checkbox/#{id}?type=card&must=#{hash.blank? ? 0 : hash["must"]}")%>" data-remote="true" class="iconRequisition icon_editor_model J_editor_checkbox">编辑</a>
  </div>
</div>