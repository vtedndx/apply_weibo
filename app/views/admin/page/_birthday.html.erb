<div class="requisitionModel J_birthday" id="div_<%= id%>" onMouseOut="in_element('<%= id%>')" onMouseOver="out_element('<%= id%>')">
	<input type="hidden" name="list[<%= id%>]type" value="birthday"/>
	<input type="hidden" name="list[<%= id%>]must" id="<%= id%>_must" value="<%= hash.blank? ? 0 : hash["must"]%>"/>
	<input type="hidden" name="list[<%= id%>]title" value="生日"/>
	<p class="requisitionLabel_text text_color">生日：</p>
	<label class="requisitionLabel" for="birthday_years">
		<select class="birthday birthday_years" name="birthday_years">
			<option>选择</option>
		</select>
		<span class="birthday_span">年</span>
	</label>
	<label class="requisitionLabel" for="birthday_month">
		<select class="birthday birthday_month" name="birthday_month">
			<option>选择</option>
		</select>
		<span class="birthday_span">月</span>
	</label>
	<label class="requisitionLabel" for="birthday_day">
		<select class="birthday birthday_day" name="birthday_day">
			<option>选择</option>
		</select>
		<span class="birthday_span">日</span>
	</label>
	<% if hash.blank? || hash["must"] == "0"%>
		<span class="required">*</span>
	<% else %>
		<span class="required" style="display: none;">*</span>
	<% end %>
	<div class="addOperating">
		<div class="requisitionModel_bg"></div>
		<span class="iconRequisition icon_move_model"></span>
		<a href="javascript:del_element('<%= id%>');" class="iconRequisition icon_delete_model J_delete_model"></a>
		<a href="<%= manager_link("/admin/page/edit_must/#{id}?type=birthday&must=#{hash.blank? ? 0 : hash["must"]}")%>" data-remote="true" class="iconRequisition icon_editor_model J_editor_birthday">编辑</a>
	</div>
</div>