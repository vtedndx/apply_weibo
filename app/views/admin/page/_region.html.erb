<div class="requisitionModel J_region" id="div_<%= id%>" onMouseOut="in_element('<%= id%>')" onMouseOver="out_element('<%= id%>')">
	<input type="hidden" name="list[<%= id%>]type" value="region"/>
	<input type="hidden" name="list[<%= id%>]must" id="<%= id%>_must" value="<%= hash.blank? ? 0 : hash["must"]%>"/>
	<input type="hidden" name="list[<%= id%>]title" value="地区"/>
	<p class="requisitionLabel_text text_color">地区：</p>
	<label class="requisitionLabel"  >
		<select class="province_select" name="province">
			<option value="不限">不限</option>
		</select>
	</label>
	<label class="requisitionLabel" >
		<select class="city_select ml20" name="city">
			<option value="不限">不限</option>
		</select>
	</label>
	<% if hash.blank? || hash["must"] == "0"%>
		<span class="required">*</span>
	<% else %>
		<span class="required" style="display: none;">*</span>
	<% end %>
	<div class="addOperating">
		<div class="requisitionModel_bg"></div>
		<span class="iconRequisition icon_move_model"></span>
		<a href="javascript:del_element('<%= id%>');" class="iconRequisition icon_delete_model J_delete_model"></a>
		<a href="<%= manager_link("/admin/page/edit_must/#{id}?type=region&must=#{hash.blank? ? 0 : hash["must"]}")%>" data-remote="true" class="iconRequisition icon_editor_model J_editor_region">编辑</a>
	</div>
</div>